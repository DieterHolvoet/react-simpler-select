    "eslint-plugin-react": "^5.2.2",
    "mocha": "^2.4.5",
    "rackt-cli": "^0.5.4",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0"
